echo "">_kaikki
for i in /tmp/vtx/1/txt/*.txt
do
    echo --------------- $i ----------------------- >>_kaikki
    cat $i >>_kaikki
    echo "--------------------------------------------------">>_kaikki
    echo "">>_kaikki
done

